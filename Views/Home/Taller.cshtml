<section class="contenido">
				<div class="contenedor">
					<div class="titulo-pagina-activa">
						<h3>Taller</h3>
					</div>
					<div class="direccion-pagina">
						<ol class="dir_menu">
							<li><a href=""><i class="fa fa-tachometer" aria-hidden="true"></i>Inicio</a></li>
							<li class="active">Taller</li>
						</ol>
					</div>
					<div class="form_contenedor">
						<div class="barra_menu">
							<a href="" id="resp-form" class="resp-form"><i class="fa fa-minus"></i></a>
						</div>
						<form action="" class="formAula" name="formAula">
							
							<div class="barra_menu_desplegar" id="barra_menu_desplegar">
								<h2>Taller</h2>
								<div class="cont_form">
									<div class="cont_row">
										<label for="" class="lbl_form">Código Taller:</label>
										<div class="codigo">
											<label for="" class="prefijo">TAL</label><input type="number" maxlength="3" oninput="if(this.value.length > this.maxLength) this.value = this.value.slice(0,this.maxLength);" onkeypress="return soloNumeros(event)" onblur="limpiaNumeros()" id="txtCodCarrera" class="txtCodCarrera" name="txtCodTaller" placeholder="Código Taller" autofocus="">
										</div>
									</div>
									<div class="cont_row">
										<label for="" class="lbl_form">Taller:</label><input type="text" onkeypress="return soloLetras(event)" onblur="limpia()" id="letras" name="txtTaller" placeholder="Taller">
									</div>
									<div class="cont_row">
										<label for="" class="lbl_form">Hora de Inicio:</label><input type="time" name="txtHoraIni" placeholder="Hora de Inicio">
									</div>
									<div class="cont_row">
										<label for="" class="lbl_form">Hora de Fin:</label><input type="time" name="txtHoraFin" placeholder="Hora de Fin">
									</div>
									<div class="cont_row">
										<label for="" class="lbl_form">Fecha:</label><input type="date" value="2018-06-12" name="txtFecha" placeholder="Fecha">
									</div>
									<div class="cont_row">
										<label for="" class="lbl_form">Código Pabellón:</label>
										<select name="cboCodPab" size=1 class="cboCampo"> 
											<option>PAB001</option>
											<option>PAB002</option>
											<option>PAB003</option>
										</select>
									</div>
									<div class="cont_row">
										<label for="" class="lbl_form">Código Aula:</label>
										<select name="cboCodAula" size=1 class="cboCampo"> 
											<option>101</option>
											<option>102</option>
											<option>103</option>
										</select>
									</div>
									<div class="cont_row">
										<label for="" class="lbl_form">Código Carrera:</label>
										<select name="cboCodCar" size=1 class="cboCampo"> 
											<option>CAR001</option>
											<option>CAR002</option>
											<option>CAR003</option>
										</select>
									</div>
									<div class="cont_row">
										<label for="" class="lbl_form">Código Evento:</label>
										<select name="cboCodEve" size=1 class="cboCampo"> 
											<option>EVE001</option>
											<option>EVE002</option>
											<option>EVE003</option>
										</select>
									</div>
									<div class="cont_btns_form">
										<input type="submit" name="btnRegistrar" value="Registrar" id="btnRegistrar" class="btnRegistrar">
										<input type="submit" name="btnModificar" value="Modificar" id="btnModificar" class="btnModificar">
										<input type="submit" name="btnEliminar" value="Eliminar" id="btnEliminar" class="btnEliminar">
									</div>
									<div class="cont_row">
										<label for="" class="lbl_form">Buscar:</label><input type="text" name="txtCriterio" placeholder="Ingrese Criterio">
									</div>
									<div class="cont_row">
										<label for="" class="lbl_form">En base a:</label>
										<select name="cboCampo" size=1 class="cboCampo"> 
											<option>IDCONFE</option>
											<option>CONFERENCIA</option>
											<option>HORA_INI</option>
											<option>HORA_FIN</option>
											<option>FECHA</option>
											<option>IDPAB</option>
											<option>IDAULA</option>
											<option>IDCARRERA</option>
											<option>IDEVENTO</option>
										</select>
									</div>
									<div class="cont_btns_form">
										<input type="submit" name="btnBuscar" value="Buscar" id="btnBuscar" class="btnBuscar">
										<input type="submit" name="btnReiniciar" value="Reiniciar" id="btnReiniciar" class="btnReiniciar">
										<input type="submit" name="btnOrdenarXID" value="Ordenar por Código" id="btnOrdenarXID" class="btnOrdenarXID">
									</div>
								</div>
							</div>
						</form>
					</div>

					<div class="cont_table">
						<table>
							<tr>
								<th>Código Taller</th>
								<th>Taller</th>
								<th>Hora de Inicio</th>
								<th>Hora de Fin</th>
								<th>Fecha</th>
								<th>Código Pabellón </th>
								<th>Código Aula</th>
								<th>Código Carrera</th>
								<th>Código Evento</th>
							</tr>
							<tr>
								<td><%=a.%></td>
								<td><%=a.%></td>
								<td><%=a.%></td>
								<td><%=a.%></td>
								<td><%=a.%></td>
								<td><%=a.%></td>
								<td><%=a.%></td>
								<td><%=a.%></td>
								<td><%=a.%></td>
							</tr>
						</table>
					</div>

				</div>
			</section>

		</div>

		<footer>
			<div class="contenedor">
				<div class="derechos-reservados">
					<p><i class="fa fa-copyright" aria-hidden="true"></i>Todos los Derechos Reservados</p>
				</div>
				<div class="derechos">
					<div class="escudo-usmp">
						<img src="imgs/escudo_usmp.png" alt="">
						<div class="info_derechos animated fadeInLeft">
							<div class="flecha-down"></div>
							<h5>Acosta Flores, Allison</h5>
							<h5>Campos Ortiz, Carlos</h5>
							<h5>Silva Cornejo, Marynadi</h5>
							<h5>Velásquez Herrera, Marcelo</h5>
						</div>
